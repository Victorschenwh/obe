  <insert id="insert" parameterType="com.dbsy.obe.pojo.Point">
    insert into point (id, introduction, requirement_id
      )
    values (#{id,jdbcType=INTEGER}, #{introduction,jdbcType=VARCHAR}, #{requirementId,jdbcType=INTEGER}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.dbsy.obe.pojo.Point">
    insert into point
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="introduction != null">
        introduction,
      </if>
      <if test="requirementId != null">
        requirement_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="introduction != null">
        #{introduction,jdbcType=VARCHAR},
      </if>
      <if test="requirementId != null">
        #{requirementId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
